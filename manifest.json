{
  "manifest_version": 3,
  "name": "Active Team Tickets",
  "version": "1.0",
  "description": "Color the logs",
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "content_scripts": [
      {
          "js": ["scripts/content.js"],
          "matches": [
              "https://www.jotform.com/admn/dashboards/*"
          ]
      }
  ],
  "permissions": [
      "activeTab",
      "scripting",
      "storage"
  ]
}
